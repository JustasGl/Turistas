        <View
            style="@style/InfoSeperator"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/telefono8"
               style="@style/InfoTelPic" />

            <ImageView
                android:id="@+id/telefono7"
                style="@style/InfoTelPic" />

            <View
               style="@style/InfoVerticalSkyriklis" />

            <ImageView
                android:id="@+id/adreso5"
                style="@style/InfoMapPic" />
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:foreground="?attr/selectableItemBackground">

            <ImageView
                android:id="@+id/google"
                style="@style/InfoMoreButton" />
        </FrameLayout>
    </LinearLayout>
</ScrollView>