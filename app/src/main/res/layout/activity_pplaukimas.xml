        <View
            style="@style/InfoSeperator"/>

        <TextView
            style="@style/InfoTitle"
            android:textSize="@dimen/contactstextsize"
            android:text="@string/kubilasPlungeTitle" />

        <TextView
            style="@style/InfoKontaktaiGrafa"
            android:text="@string/kontaktai"
            />

        <LinearLayout
           style="@style/matchnwrap"
            android:orientation="horizontal">

            <TextView
                style="@style/InfoTelNr"
                android:text="@string/kubilasPlungeTelNr"
                />

            <TextView
               style="@style/InfoKontaktai"
                android:text="@string/kubilasPlungeAddres"
                />
        </LinearLayout>

        <View
            style="@style/InfoSeperator"/>

        <LinearLayout
            style="@style/matchnwrap">

            <ImageView
                android:id="@+id/telefono2"
               style="@style/InfoTelPic" />

            <View
               style="@style/InfoVerticalSkyriklis" />

            <ImageView
                android:id="@+id/adreso2"
