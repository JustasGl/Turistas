<resources>

    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">#EF5350</item>
        <item name="colorPrimaryDark">#D32F2F</item>
        <item name="colorAccent">#EF9A9A</item>
        <item name="colorButtonNormal">#EF9A9A</item>
    </style>

    <style name="FullscreenTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:actionBarStyle">@style/FullscreenActionBarStyle</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="metaButtonBarStyle">?android:attr/buttonBarStyle</item>
        <item name="metaButtonBarButtonStyle">?android:attr/buttonBarButtonStyle</item>
    </style>

    <style name="FullscreenActionBarStyle" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>

    <style name="stroke">
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">3</item>
        <item name="android:shadowDy">3</item>
        <item name="android:shadowRadius">3</item>
    </style>

    <style name="ToolBarStyle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:textColorSecondary">@android:color/white</item>
        <item name="actionMenuTextColor">@android:color/white</item>
        <item name="colorPrimary">#ffffff</item>
        <item name="colorPrimaryDark">#ffffff</item>
        <item name="colorAccent">#ffffff</item>
    </style>

    <style name="ToolBarStyledark" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:textColorSecondary">@android:color/white</item>
        <item name="actionMenuTextColor">@android:color/white</item>
        <item name="colorPrimary">#ffffff</item>
        <item name="colorPrimaryDark">#ffffff</item>
        <item name="colorAccent">#ffffff</item>
        <item name="android:textColorPrimary">@android:color/black</item>
    </style>
   <style name="matchnmatch">
       <item name="android:layout_width">match_parent</item>
       <item name="android:layout_height">match_parent</item>
   </style>
    <style name="matchnwrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="InfoTitle" parent="matchnmatch">
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">@dimen/titletextsize</item>
    </style>
    <style name="InfoSeperator">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/seperatorheight</item>
        <item name="android:layout_margin">@dimen/midmargin</item>
        <item name="android:background">@color/skyriklis</item>
    </style>
    <style name="InfoKontaktaiGrafa">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/marginmidplus</item>
        <item name="android:layout_marginTop">@dimen/midmargin</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">@dimen/contactstextsize</item>
        <item name="android:text">@string/kontaktai</item>
    </style>
    <style name="InfoTelNr">
        <item name="android:layout_width">@dimen/weightedLayoutWidth</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/midmargin</item>
        <item name="android:layout_marginStart">@dimen/midmargin</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/juoda</item>
        <item name="android:textSize">@dimen/NumberTextSize</item>
    </style>
    <style name="InfoKontaktai">
        <item name="android:layout_width">@dimen/weightedLayoutWidth</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/midmargin</item>
        <item name="android:layout_marginLeft">@dimen/midmargin</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/juoda</item>
        <item name="android:textSize">@dimen/adressSize</item>
        <item name="android:layout_marginRight">@dimen/midmargin</item>
    </style>
    <style name="InfoPic">
        <item name="android:layout_width">@dimen/picsize</item>
        <item name="android:layout_height">@dimen/picsize</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">?attr/selectableItemBackground</item>
    </style>
    <style name="InfoTelPic" parent="InfoPic">
        <item name="android:src">@drawable/telnr</item>
    </style>
    <style name="InfoMapPic" parent="InfoPic">
        <item name="android:src">@drawable/adresas</item>
    </style>
    <style name="InfoVerticalSkyriklis">
        <item name="android:layout_width">@dimen/seperatorheight</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/skyriklis</item>
    </style>
    <style name="InfoMoreButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/heightofMoreButton</item>
        <item name="android:layout_marginTop">@dimen/marginmidplus</item>
        <item name="android:background">@color/backgroundofMoreButton</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:src">@drawable/gsearch</item>
    </style>
    <style name="InfoDescription" parent="matchnwrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/midmargin</item>
        <item name="android:layout_marginLeft">@dimen/midmargin</item>
        <item name="android:textColor">@color/juoda</item>
        <item name="android:textSize">@dimen/adressSize</item>
    </style>
    <style name="InfoImageSlider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/ImageSLiderHeight</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>
    <style name="ImageDots">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingBottom">@dimen/marginLittle</item>
    </style>
    <style name="wrapnwrap">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="InfoPlaceName" parent="matchnwrap">
        <item name="android:layout_marginTop">@dimen/midmargin</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">@dimen/NameOfAPlace</item>
    </style>
    <style name="GaleryTitle" parent="matchnwrap">
        <item name="android:layout_marginBottom">@dimen/GaleryTitleMargins</item>
        <item name="android:layout_marginLeft">@dimen/GaleryTitleMargins</item>
        <item name="android:layout_marginRight">@dimen/GaleryTitleMargins</item>
        <item name="android:layout_marginTop">?attr/actionBarSize</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/GaleryTitleColor</item>
        <item name="android:textSize">@dimen/contactstextsize</item>
    </style>
    <style name="GaleryMapButton" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/GaleryMapButtonHeight</item>
        <item name="android:background">@color/GaleryMapButtonColor</item>
        <item name="android:paddingBottom">@dimen/GaleryMappButtonPadding</item>
        <item name="android:paddingTop">@dimen/GaleryMappButtonPadding</item>
        <item name="android:src">@drawable/adresas</item>
    </style>
    <style name="GaleryMoreButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/GaleryMapButtonHeight</item>
        <item name="android:background">@color/backgroundofMoreButton</item>
        <item name="android:src">@drawable/gsearch</item>
    </style>
    <style name="GaleryDescription" parent="matchnwrap">
       <item name="android:layout_margin">@dimen/GaleryMappButtonPadding</item>
        <item name="android:layout_marginBottom">@dimen/marginmidplus</item>
        <item name="android:textAlignment">textStart</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="GaleryRight">
        <item name="android:layout_width">@dimen/ArrowWidth</item>
        <item name="android:layout_height">@dimen/ArrowHeight</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:padding">@dimen/marginmidplus</item>
    </style>
    <style name="GaleryLeft" parent="GaleryRight">
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentEnd">false</item>
        </style>
    <style name="MainActivityButtons">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/HeightOfMainActivityButtons</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
        <item name="android:background">@color/white</item>
        <item name="android:scaleType">center</item>
    </style>
    <style name="MainActivityMarsrutai">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/MainActivityMarsrutaiButton</item>
        <item name="android:layout_marginTop">@dimen/seperatorheight</item>
        <item name=" android:background">@color/white</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
    </style>
    <style name="MainActivityTitles" parent="matchnwrap">
       <item name="android:layout_alignParentBottom">true</item>
        <item name="android:background">@color/MainActivityTitleBacground</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/MainActivityTitlecolor</item>
        <item name="android:textSize">@dimen/MainActivityTitle</item>
    </style>
    <style name="MainActivityButtonLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/MainActivityButtonLayoutHeight</item>
    </style>
</resources>
